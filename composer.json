{
    "name": "emilmoe/eloquence",
    "type": "extension",
    "description": "Extending the Laravel Eloquent framework.",
    "license": "proprietary",
    "authors": [{
        "name": "Emil Moe",
        "email": "emil@moegroup.dk",
        "homepage": "https://moegroup.dk"
    }],
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "php": "~7.1|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "EmilMoe\\Eloquence\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmilMoe\\Eloquence\\EloquenceServiceProvider"
            ]
        },
        "emilmoe": {
            "autoloader": [
            ]
        }
    }
}
